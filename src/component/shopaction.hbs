{{#with service}}
<section class="my-5">
    <div class="card text-bg-success text-center">
        <div class="card-body">
            <h2>{{title}}</h2>
            <p>{{description}}</p>
            {{#each buttons}}
            <a href={{link}} class="btn btn-primary">{{text}}</a>
            {{/each}}
        </div>
    </div>
</section>
{{/with}}